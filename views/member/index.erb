
<div>
  <h1>Members Page</h1>

  <div class="table-container">

    <table>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Membership Type</th>
      </tr>
      <% for member in @members %>
      <tr>
        <td><%= member.first_name %></td>
        <td><%= member.last_name %></td>
        <td><%= member.membership_type %></td>
        <td><form action="/members/<%= member.id%>">
          <input type="submit" value="View Member"/>
        </form></td>
      </tr>


      <% end %>
    </table>
  </div>

  <form class="button-forms" method ="get" action="/members/new">
    <input type="submit" value="New Member"/>
  </form>

</div>
