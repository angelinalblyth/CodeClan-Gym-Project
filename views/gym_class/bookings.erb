
<h1>Members Booked onto <%= @gym_class.title %></h1>

<div class ="table-container">

  <table>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Membership Type</th>
    </tr>
    <% for member in @members %>
    <tr>
      <td><%= member.first_name %></td>
      <td><%= member.last_name %></td>
      <td><%= member.membership_type %></td>
    </tr>
    <% end %>
  </table>

</div>

<form style="padding-left: 2.5%;"action="/gym_classes/<%= @gym_class.id %>/book_class">
  <input type="submit" value="Book someone else onto the class"/>
</form>
